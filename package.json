{
  "name": "bookmycourt",
  "version": "1.0.0",
  "description": "Premium online sports facility booking platform with 3D immersive experience",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "create-admin": "node server/scripts/createAdminAtlas.js"
  },
  "keywords": [
    "sports",
    "booking",
    "3D",
    "react",
    "threejs",
    "mongodb"
  ],
  "author": "BookMyCourt Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.7",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
